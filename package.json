  "author": "Mobelux, LLC",
  "repository": "http://github.com/Mobelux/graphdux",
  "license": "MIT",
  "dependencies": {
    "humps": ">=1.1.0",
    "lokka": ">= 1.7.0",
    "lokka-transport-http": ">= 1.4.0"
  },
  "optionalDependencies": {
    "normalizr": ">= 2.0.0",
    "react": ">= 15.0.0",
    "react-router": ">= 2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "expect": "^1.20.1",
    "mocha": "^3.1.2"
  }
