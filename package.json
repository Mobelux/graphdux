{
  "name": "graphdux",
  "version": "0.1.0",
  "description": "A graphql/redux integration toolkit",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha test/*.spec.js --compilers js:babel-register",
    "lint": "eslint --ext .js ./lib",
    "clean": "rm -rf ./dist",
    "compile": "npm run clean && babel ./lib --out-dir ./dist",
    "build": "npm run lint && npm run test && npm run compile"
